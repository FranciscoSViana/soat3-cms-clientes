{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Cliente Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "nome": {
      "type": "string"
    },
    "cpf": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "telefone": {
      "type": "string",
      "pattern": "^[0-9]{10,11}$"
    }
  },
  "required": [
    "id",
    "nome",
    "cpf",
    "email",
    "telefone"
  ]
}
